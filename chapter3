3.10
def dfs(G,u):
    visited = [False]*n
    stack = [u]
    clock = 0
    prev[u] = clock
    clock+=1
    while stack:
        u = stack.pop(-1)
        visited[u] = True
        post[u] = clock
        clock+=1
        for v in G[u]:
            if not visited[v]:
                stack.append(v)
                prev[v]= clock
                clock+=1

